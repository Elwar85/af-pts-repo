<?xml version="1.0"?>
<!--Phoronix Test Suite v5.0.1 (Khanino)-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>AF_Speedtest</Title>
    <AppVersion>1.0.1g</AppVersion>
    <Description>This is an implementation of https://github.com/sivel/speedtest-cli for Phoronix Test Suite to measure HTTP upload and download speeds</Description>
    <ResultScale>Mbits per second</ResultScale>
    <Proportion>HIB</Proportion>
    <SubTitle>Speedtest.net HTTP Performance</SubTitle>
    <TimesToRun>6</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>0.1.0</Version>
    <SupportedPlatforms>Linux, Solaris, BSD, MacOSX</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Network</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>python</ExternalDependencies>
    <EnvironmentSize>69</EnvironmentSize>
    <ProjectURL>http://www.openssl.org/</ProjectURL>
    <Maintainer>James Gartrell</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <AllowCacheShare>TRUE</AllowCacheShare>
    </Default>
    <Option>
      <DisplayName>Type</DisplayName>
      <Identifier>run-type</Identifier>
      <Menu>
        <Entry>
          <Name>Ping</Name>
          <Value>Ping</Value>
        </Entry>
        <Entry>
          <Name>Download</Name>
          <Value>Download</Value>
        </Entry>
        <Entry>
          <Name>Upload</Name>
          <Value>Upload</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
