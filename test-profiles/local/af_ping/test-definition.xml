<?xml version="1.0"?>
<!--Phoronix Test Suite v5.0.1 (Khanino)-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>AF_ping</Title>
    <AppVersion>0.1.0</AppVersion>
