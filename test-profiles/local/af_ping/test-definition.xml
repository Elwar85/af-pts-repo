    <ResultScale>milliseconds</ResultScale>
    <Proportion>LIB</Proportion>
    <SubTitle>Ping Latency Test</SubTitle>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>0.1.0</Version>
    <SupportedPlatforms>Linux, Solaris, BSD, MacOSX</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Network</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies></ExternalDependencies>
    <EnvironmentSize>1</EnvironmentSize>
    <ProjectURL></ProjectURL>
    <Maintainer>James Gartrell</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <AllowCacheShare>FALSE</AllowCacheShare>
    </Default>
    <Option>
      <DisplayName>Type</DisplayName>
      <Identifier>run-type</Identifier>
      <Menu>
        <Entry>
          <Name>www.google.com</Name>
          <Value>-c 10 www.google.com</Value>
        </Entry>
        <Entry>
          <Name>www.facebook.com</Name>
          <Value>-c 10 www.facebook.com</Value>
        </Entry>
        <Entry>
          <Name>sping-sj.anchorfree.net</Name>
          <Value>-c 10 sping-sj.anchorfree.net</Value>
        </Entry>
        <Entry>
          <Name>sping-ny.anchorfree.net</Name>
          <Value>-c 10 sping-ny.anchorfree.net</Value>
        </Entry>
        <Entry>
          <Name>sping-frk.anchorfree.net</Name>
          <Value>-c 10 sping-frk.anchorfree.net</Value>
        </Entry>
        <Entry>
          <Name>sping-miami.anchorfree.net</Name>
          <Value>-c 10 sping-miami.anchorfree.net</Value>
        </Entry>
        <Entry>
          <Name>sping-zur.anchorfree.net</Name>
          <Value>-c 10 sping-zur.anchorfree.net</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
