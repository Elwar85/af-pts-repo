<?xml version="1.0"?>
<!--Phoronix Test Suite v5.0.1 (Khanino)-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>AF_IOping_Sequential</Title>
    <AppVersion>0.9</AppVersion>
    <Description>IOping measures disk I/O latency and throughput</Description>
    <ResultScale>Bytes per second</ResultScale>
    <Proportion>HIB</Proportion>
    <SubTitle>Disk Sequential Speed</SubTitle>
    <TimesToRun>6</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>0.1.0</Version>
    <SupportedPlatforms>Linux, Solaris, BSD, MacOSX</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Processor</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>build-utilities</ExternalDependencies>
    <EnvironmentSize>1</EnvironmentSize>
    <ProjectURL>https://github.com/koct9i/ioping</ProjectURL>
    <Maintainer>James Gartrell</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <AllowCacheShare>FALSE</AllowCacheShare>
    </Default>
    <Option>
      <DisplayName>Type</DisplayName>
      <Identifier>run-type</Identifier>
      <Menu>
        <Entry>
          <Name>Volume sequential speed /tmp</Name>
          <Value>-B -RL /tmp</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
